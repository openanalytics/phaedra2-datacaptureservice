{
    "name": "Folder capture configuration",
    "identifyMeasurements": {
        "scriptId": "identify.txt.measurements",
        "measurementPattern": "\\d+$",
        "barcodePattern": "\\d{2,}"
    },
    "gatherWellData": {
        "scriptId": "gather.txt.welldata",
        "parserId": "txt.welldata.dir.parser",
        "filePattern": "[Ww]ell[A-Z]+[0-9]+_WellLevel\\.txt",
        "dataLineSeparator": "\n",
        "columnSeparator": "\t"
    },
    "gatherSubwellData": {
        "scriptId": "gather.txt.subwelldata",
        "parserId": "txt.subwelldata.dir.parser",
        "filePattern": "[Ww]ell[A-Z]+[0-9]+_Details\\.txt",
        "dataLineSeparator": "\n",
        "columnSeparator": "\t"
    }
}
